{
  "name": "03_vue3_setup_tutor",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src*/**/*.js": [
      "prettier --config ./.prettierrc.js --write",
      "eslint",
      "git add"
    ],
    "src*/**/*.ts": [
      "prettier --config ./.prettierrc.js --write",
      "eslint",
      "git add"
    ],
    "src*/**/*.json": [
      "prettier --config ./.prettierrc.js --write",
      "eslint",
      "git add"
    ],
    "src*/**/*.vue": [
      "prettier --config ./.prettierrc.js --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^6.6.2",
    "ant-design-vue": "3.0.0-alpha.12",
    "axios": "^0.21.1",
    "unplugin-vue-components": "^0.17.2",
    "vue": "^3.2.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "postcss-less": "^5.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vite": "^2.6.14",
    "vue-tsc": "^0.3.0"
  }
}
